{
  "settings": {
    "enableRemixiconsLine": true,
    "enableRemixiconsFill": false,
    "enableFontawesomeFill": false,
    "enableFontawesomeLine": false,
    "enableFontawesomeBrands": false,
    "fontSize": 16,
    "iconColor": null,
    "extraPadding": {
      "top": 2,
      "right": 2,
      "bottom": 2,
      "left": 2
    },
    "migrated": 4,
    "iconPacksPath": ".obsidian/plugins/obsidian-icon-folder/icons",
